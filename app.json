{
  "expo": {
    "name": "BlackBox",
    "slug": "BlackBox",
    "version": "1.2.1",
    "orientation": "default",
    "icon": "./assets/blackBox.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/blackBox.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.rainwater.rainwater",
      "versionCode": 1,
      "permissions" : []
    },
    "web": {
      "favicon": "./assets/blackBox.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}
